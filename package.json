{
  "name": "universal-react-app",
  "version": "0.0.1",
  "description": "Universal Express, React, React-router starter app",
  "author": "Natasha Koh Sze Sze <github.com/NatashaKSS>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NatashaKSS/universal-react-app.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js && node server.js --ext js,jsx",
    "build:dev": "webpack --config webpack.config.js",
    "start": "cross-env NODE_ENV=development nodemon server.js --exec babel-node"
  },
  "main": "server.js",
  "keywords": [
    "nodejs",
    "redux",
    "universal react",
    "isomorphic react",
    "react",
    "serverside rendering"
  ],
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "express": "^4.13.4",
    "path": "^0.12.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.0",
    "redux": "^3.7.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.0.1",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "license": "MIT"
}
